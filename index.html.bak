<!DOCTYPE html>
<html>
<head>
  <title>Twilio Video - Video Quickstart</title>
  <link rel="stylesheet" href="site.css">
</head>
<body>
  <div id="remote-media"></div>
  <div id="controls">
    <div id="preview">
      <p class="instructions">Hello Beautiful</p>
      <div id="local-media"></div>
      <button id="button-preview">Preview My Camera</button>
    </div>
    <div id="room-controls">
      <p class="instructions">Room Name:</p>
      <input id="room-name" type="text" placeholder="Enter a room name" />
      <button id="button-join">Join Room</button>
      <button id="button-leave">Leave Room</button>
	  
      <button id="button-mute-audio">mute Audio</button>
      <button id="button-unmute-audio">unmute Audio</button>
	  
	  <button id="button-mute-video">mute video</button>
      <button id="button-unmute-video">unmute video</button>
	  
	   <button id="button-share-screen">Share</button>
    </div>
    <div id="log"></div>
  </div>

  <script src="//sdk.twilio.com/js/video/releases/2.15.3/twilio-video.min.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
  <script src="quickstart.js"></script>
</body>
</html>
